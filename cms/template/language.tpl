<p>&nbsp;</p>
<table width="100%" border="0" cellspacing="0" cellpadding="2" class="table_list">
  <tr>
    <th class="th-checkbox firstColumn">{lang.code}</th>
    <th class="th-name">{lang.language}</th>
    <th class="th-status {hide_language_access}">{lang.default} </th>
    <th class="th-status {hide_language_access}">{lang.status}</th>
    <th class="th-action lastColumn">{lang.edit}</th>
  </tr>
  <!--BASIC language-->
  <tr>
    <td class="th-checkbox firstColumn">{language.ln}</td>
    <td class="th-name">{language.ln_name}</td>
    <td class="th-status {hide_language_access}"><a href="?mod={module}&act=default&ln={language.ln}" title="{lang.update_default}"><img src="images/icons_default/status{language.is_default}.gif" border="0"></a></td>
    <td class="th-status {hide_language_access}"><a href="?mod={module}&act=active&ln={language.ln}" title="{lang.update_status}"><img src="images/icons_default/status{language.active}.gif" border="0"></a></td>
    <td class="th-action lastColumn"><span><a href="?mod={module}&act=update&ln={language.ln}" title="{lang.edit}"><img src="images/icons_default/edit.png" /></a></span></td>
  </tr>
  <!--BASIC language-->
</table>
